{
  "name": "ttcsupportbot",
  "version": "1.0.0",
  "description": "Support bot for TTC",
  "main": "bot.ts",
  "scripts": {
    "start:prod": "cross-env APP_TYPE=PROD node --no-warnings --loader ts-node/esm bot.ts",
    "start": "nodemon src/bot.ts",
    "start:two": "node --no-warnings --loader ts-node/esm ./target/bot.js",
    "start:dev": "cross-env APP_TYPE=DEV node --trace-warnings --loader ts-node/esm bot.ts",
    "build": "rimraf target && tsc && npm run copy",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add target",
    "copy": "copyfiles -u 1 source/src/**/* target/ && copyfiles -u 1 source/log/**/* target/ && copyfiles -u 1 source/log/**/* target/ && copyfiles -u 1 source/package.json target/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "repository": {
    "url": "https://github.com/drykov-ttc/ttcNSA.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/archiver": "^5.3.2",
    "@types/bunyan": "^1.8.8",
    "@types/cli-table": "^0.3.1",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.5.0",
    "@types/winston-syslog": "^2.4.0",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.0.1",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "@grammyjs/conversations": "^1.1.2",
    "@grammyjs/files": "^1.0.4",
    "@grammyjs/runner": "^2.0.3",
    "@grammyjs/storage-file": "^2.3.0",
    "@loskir/grammy-markup": "^1.1.0",
    "@lqmanh/netsnmp": "^0.1.1",
    "@types/ejs": "^3.1.2",
    "@types/log4js": "^2.3.5",
    "@types/netmask": "^2.0.2",
    "@types/nodemailer": "^6.4.9",
    "@types/ping": "^0.4.1",
    "@types/python-shell": "^1.0.3",
    "@types/snmp-native": "^1.2.2",
    "@types/underscore": "^1.11.6",
    "@types/winston": "^2.4.4",
    "archiver": "^6.0.0",
    "axios": "^1.4.0",
    "child_process": "^1.0.2",
    "cli-table": "^0.3.11",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "grammy": "^1.17.2",
    "moment": "^2.29.4",
    "mongodb": "^5.7.0",
    "mongoose": "^7.4.0",
    "net-snmp": "^3.9.7",
    "netmask": "^2.0.2",
    "nodemailer": "^6.9.4",
    "ping": "^0.4.4",
    "python-shell": "^5.0.0",
    "reflect-metadata": "^0.1.13",
    "snmp-native": "^1.2.0",
    "snmp-sysobjectid": "^1.0.3",
    "table": "^6.8.1",
    "underscore": "^1.13.6",
    "util": "^0.12.5",
    "winston": "^3.10.0",
    "winston-syslog": "^2.7.0"
  }
}
