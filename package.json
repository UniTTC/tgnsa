{
  "name": "ttcsupportbot",
  "version": "1.0.0",
  "description": "Support bot for TTC",
  "main": "bot.ts",
  "scripts": {
    "start:prod": "cross-env APP_TYPE=PROD node --no-warnings --loader ts-node/esm ./src/bot.ts",
    "start": "nodemon --inspect --no-warnings ./src/bot.ts",
    "start:two": "node --trace-warnings --loader ts-node/esm ./dist/bot.js",
    "start:dev": "cross-env APP_TYPE=DEV node --trace-warnings --loader ts-node/esm ./src/bot.ts",
    "build": "rimraf target && tsc && npm run copy",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add target",
    "copy": "cp -rf src/src/* dist/src/ && copyfiles -u 1 source/package.json dist/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "debug": "node --inspect --loader ts-node/esm ./dist/index.js",
    "debug:watch": "nodemon --inspect --no-warnings src/index.ts"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "repository": {
    "url": "https://github.com/drykov-ttc/ttcNSA.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/archiver": "^5.3.4",
    "@types/bunyan": "^1.8.8",
    "@types/cli-table": "^0.3.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.4",
    "@types/ping": "^0.4.4",
    "@types/winston-syslog": "^2.4.3",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.2",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@grammyjs/conversations": "^1.1.2",
    "@grammyjs/files": "^1.0.4",
    "@grammyjs/runner": "^2.0.3",
    "@grammyjs/storage-file": "^2.4.1",
    "@loskir/grammy-markup": "^1.1.0",
    "@lqmanh/netsnmp": "^0.1.1",
    "@pm2/io": "^5.0.2",
    "@types/ejs": "^3.1.5",
    "@types/log4js": "^2.3.5",
    "@types/netmask": "^2.0.5",
    "@types/nodemailer": "^6.4.14",
    "@types/python-shell": "^1.0.3",
    "@types/snmp-native": "^1.2.5",
    "@types/underscore": "^1.11.15",
    "@types/winston": "^2.4.4",
    "archiver": "^6.0.1",
    "axios": "^1.6.2",
    "canvas": "^2.11.2",
    "child_process": "^1.0.2",
    "cli-table": "^0.3.11",
    "cli-table3": "^0.6.3",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "firebase-admin": "^12.0.0",
    "grammy": "^1.19.2",
    "moment": "^2.29.4",
    "mongodb": "^5.9.2",
    "mongoose": "^7.6.7",
    "net-snmp": "^3.9.7",
    "netmask": "^2.0.2",
    "node-debug": "^0.1.0",
    "nodemailer": "^6.9.7",
    "ping": "^0.4.4",
    "python-shell": "^5.0.0",
    "reflect-metadata": "^0.1.14",
    "snmp-native": "^1.2.0",
    "snmp-sysobjectid": "^1.0.3",
    "table": "^6.8.1",
    "tsconfig-paths": "^3.15.0",
    "underscore": "^1.13.6",
