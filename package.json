{
  "name": "ttcsupportbot",
  "version": "1.0.0",
  "description": "Support bot for TTC",
  "main": "bot.ts",
  "scripts": {
    "start:prod": "cross-env APP_TYPE=PROD node --no-warnings --loader ts-node/esm ./src/bot.ts",
    "start": "cross-env APP_TYPE=DEV NOWEB=false PORT=443 nodemon --inspect --no-warnings ./src/bot.ts",
    "start:two": "node --trace-warnings --loader ts-node/esm ./dist/bot.js",
    "start:dev": "cross-env APP_TYPE=DEV node --trace-warnings --loader ts-node/esm ./src/bot.ts",
    "build": "rimraf target && tsc && npm run copy",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add target",
    "copy": "cp -rf src/src/* dist/src/ && copyfiles -u 1 source/package.json dist/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "debug": "node --inspect --loader ts-node/esm ./dist/index.js",
    "debug:watch": "nodemon --inspect --no-warnings src/index.ts"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "repository": {
    "url": "https://github.com/drykov-ttc/ttcNSA.git"
  },
  "author": "",
  "license": "ISC",
